<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aashish.app.kharcha">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_square"
        android:label="Kharcha"
        android:roundIcon="@mipmap/ic_launcher_square"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.aashish.app.kharcha.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aashish.app.kharcha.LoginActivity" />
        <activity android:name="com.aashish.app.kharcha.SignUpActivity" />
        <activity
            android:name="com.aashish.app.kharcha.HomeActivity"
            android:label="@string/title_activity_dashboard"></activity>
    </application>

</manifest>